.footer {
    clear: both;
    background: #FFFFFF;
    padding: 10px;
    text-align: center;
    color: #00B2D3;
    font-size: 11px;
    border-top: 4px solid #003660;
    margin-top: 10px;
}

    .footer span {
        display: block;
        color: #96999D;
        font-size: 10px;
    }
/* Entry Section */
.entryOuterDiv {
    width: 100%;
    padding: 0px;
}

.entryArea {
    margin-left: auto;
    margin-right: auto;
    width: 600px;
}

.column-center {
    border-width: 0px;
    border-style: solid;
    border-color: Gray;
}
/* Entry page */
.entryArea ul.entryRow {
    list-style-type: none;
    text-align: center;
    margin: 0px;
    padding: 0px;
    overflow: auto;
}

    .entryArea ul.entryRow li {
        float: left;
        padding-left: 5px;
        padding-right: 5px;
        width: 250px;
        margin-left: auto;
        margin-right: auto;
        line-height: 32px;
        text-align: left;
    }

        .entryArea ul.entryRow li.text {
            width: 300px;
            text-align: right;
            font-size: 8px;
            font-family: Arial;
        }

            .entryArea ul.entryRow li.text span {
                color: #ff0000;
            }

        .entryArea ul.entryRow li.inputArea {
            width: 600px;
            text-align: left;
            font-size: 12px;
        }

        .entryArea ul.entryRow li.selectArea {
            width: 350px;
            text-align: left;
            font-size: 12px;
        }

            .entryArea ul.entryRow li.selectArea select {
                font-size: 12px;
                width: 202px;
                height: 22px;
            }

        .entryArea ul.entryRow li.col2 {
            width: 360px;
            text-align: left;
            font-size: 12px;
        }

/* input styles */
.textInput {
    width: 200px;
    font-size: 11px;
    height: 18px;
}

.textInputSpan1 {
    width: 30px;
    font-size: 11px;
    height: 18px;
}

.textInputAge {
    width: 30px;
    font-size: 11px;
    height: 18px;
}

