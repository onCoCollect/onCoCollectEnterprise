a.link_btn {
    display: block;
    float: left;
    padding: 0px 15px;
    background-color: #ef800e;
    height: 22px;
    text-decoration: none;
    font-family: Verdana;
    font-size: 13px;
    color: #ffffff;
    font-weight: bold;
    cursor: pointer;
    line-height: 22px;
    text-align: center;
    margin-right: 15px;
    -moz-border-radius: 5px;
    border-radius: 5px;
}

    a.link_btn:hover {
        display: block;
        float: left;
        background-color: #d76f06;
        text-decoration: none;
        -moz-border-radius: 5px;
        border-radius: 5px;
    }
