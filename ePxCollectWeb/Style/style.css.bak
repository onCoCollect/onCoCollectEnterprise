/*** Onco Collect Portal Stylesheet ***/

/* General */

body::-webkit-scrollbar {
    width: 5px;
}

body::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 2px rgba(0,0,0,0.3);
}

body::-webkit-scrollbar-thumb {
    background-color: darkgrey;
    outline: 1px solid slategrey;
}

.clear {
    clear: both;
}

html {
    background-color:#fff;
}

.container {
    margin: 0 0;
    width: 100%;
    background-color: #fff;
    padding: 0;
    margin-top: 0px;
    height: 100%;
    /*
    margin: 0 auto;
    width: 95%;
    background-color: #fff;
    padding: 10px;
    margin-top: 30px;*/
}

img {
    border-style: none;
    border-color: inherit;
    border-width: 0px;
    height: auto;
    margin-top: 0px;
    width: auto;
}

.leftFloat {
    float: left;
}

.rightFloat {
    float: right;
}

a {
    text-decoration: none;
    outline: none;
}

    a.generalLinks {
        color: #f48310;
    }

        a.generalLinks:active {
            color: #f48310;
        }

        a.generalLinks:hover {
            color: #f48310;
            text-decoration: underline;
        }

.LoginLinks {
    padding-top: 5px;
    color: #ffffff;
    font-size: 13px;
    line-height: 23px;
}

    .LoginLinks a:link {
        color: #f9d7b4;
        text-decoration: none;
    }

    .LoginLinks a:visited {
        text-decoration: none;
    }

    .LoginLinks a:hover {
        text-decoration: underline;
    }

/* welcome section */
.welcome {
    font-size: 12px;
    padding-top: 0px;
    text-align: right;
}

    .welcome a:link {
        color: #0D0D0D;
        font-size: 12px;
        font-weight: bold;
        text-decoration: none;
    }

    .welcome a:visited {
        text-decoration: none;
    }

    .welcome a:hover {
        text-decoration: underline;
    }

/* Menu ` */
#navigation {
    background-color: #92A117;
    background-image: url(../images/sidebar_bg.jpg);
    background-position: top left;
    background-repeat: no-repeat;
    width: 200px;
    padding: 12px 0 14px 13px;
    font-family: 'Trebuchet MS', sans-serif;
    font-size: 12px;
    color: #0D0D0D;
    border-bottom: 1px solid #fff;
}

    #navigation li {
        list-style-type: none;
        padding: 0 0 2px 0;
        line-height: 28px;
    }

    #navigation ab {
        color: #0D0D0D;
        text-decoration: none;
        font-weight: bold;
        display: block;
        width: 179px;
        line-height: 28px;
        background-image: url(../images/li_bg.jpg);
        background-position: top left;
        background-repeat: no-repeat;
        padding: 0 0 0 30px;
    }

    #navigation a:visited {
        text-decoration: none;
    }

    #navigation a:hover {
        text-decoration: underline;
    }

#help {
    height: 38px;
    line-height: 16px;
    margin: 21px 0 0 0;
    padding: 9px 0 0 50px;
    background-image: url(../images/img_help.png);
    background-position: top left;
    background-repeat: no-repeat;
}

.helpContent {
    cursor: pointer;
    padding-top: 5px;
    font-size: 13px;
    font-weight: bold;
}

/* login */

.loginArea {
    background-color: #92A117;
    background-image: url(../images/sidebar_bg.jpg);
    background-position: top left;
    background-repeat: no-repeat;
    width: 220px;
    height: 308px;
    padding: 12px 0 14px 13px;
    font-family: 'Trebuchet MS', sans-serif;
    font-size: 12px;
    color: #0D0D0D;
    border-bottom: 1px solid #fff;
}

    .loginArea h1 {
        margin: 0;
        padding-bottom: 10px;
        color: #fefefe;
        font-size: 20px;
        font-weight: bold;
    }

    .loginArea ul.dataRow {
        list-style-type: none;
        text-align: center;
        margin: 0px;
        padding: 0px;
        overflow: hidden;
    }

        .loginArea ul.dataRow li {
            float: left;
            padding-right: 5px;
            width: 200px;
            margin-left: auto;
            margin-right: auto;
            line-height: 20px;
            text-align: left;
        }

            .loginArea ul.dataRow li.text {
                width: 200px;
                text-align: left;
                font-size: 13px;
                color: #ffffff;
                padding-top: 10px;
            }

                .loginArea ul.dataRow li.text span {
                    color: #ff0000;
                }

            .loginArea ul.dataRow li.inputArea {
                width: 220px;
                text-align: left;
                font-size: 12px;
            }

/* main content */
#contentMain {
    padding: 0 0 1px 0;
    overflow: auto;
}

#sidebar {
    padding: 0 0 0 0;
    width: 100%;
}

#insideHome {
    width: 900px;
    padding: 15px 0 0 17px;
    float: left;
}

    #insideHome h1 {
        font-size: 18px;
        font-weight: bold;
    }

    #insideHome p {
        font-family: 'Trebuchet MS', sans-serif;
        font-size: 13px;
        color: #444444;
        padding: 15px 0 20px 0;
        line-height: 20px;
        text-align: justify;
    }

#inside {
    padding: 5px 0 0 20px;
    float: left;
    width: 700px;
}

    #inside h1 {
        margin: 0;
        padding-bottom: 10px;
        font-size: 16px;
        font-weight: bold;
    }

.info {
    font-family: 'Trebuchet MS', sans-serif;
    font-size: 13px;
    color: #444444;
    padding: 7px 10px 30px 10px;
    overflow: hidden;
    float: left;
    width: 582px;
    background-color: #F5F5F5;
}

    .info p {
        width: 680px;
        float: left;
        padding: 0 0 12px 0;
        text-align: justify;
    }

/* footer */
