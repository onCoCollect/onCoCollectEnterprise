.button {
    padding: 7px 14px;
    border: 1px solid #035DC4;
    background-image: linear-gradient(bottom, #095DB7 0, #76A8E0 100%);
    background-image: linear-gradient(to bottom, #76A8E0 0, #095DB7 100%);
    background-color: #035DC4;
    color: #fff;
    text-shadow: 1px 1px 0 #2B6BB5;
    text-decoration: none;
    border-radius: 5px;
    box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.3);
    margin: 5px;
}

    .button:hover {
        background: none #76A8E0;
        border-color: #035DC4;
        text-shadow: none;
        color: #4A463B;
        margin: 5px;
    }

    .button:active {
        border-top-color: #76A8E0;
        background: #095DB7;
        color: #fff;
        margin: 5px;
    }

/* Button Disabled*/
.buttonDisable {
    -moz-box-shadow: inset 0px 1px 0px 0px #ffffff;
    -webkit-box-shadow: inset 0px 1px 0px 0px #ffffff;
    box-shadow: inset 0px 1px 0px 0px #ffffff;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#f6f6f6',GradientType=0);
    -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    border-radius: 3px;
    border: 1px solid #dcdcdc;
    cursor: pointer;
    font-family: Verdana;
    font-size: 11px;
    font-weight: bold;
    padding: 5px;
    text-decoration: none;
    width: 100px;
}



