            function ValidateTextBoxForDataTypeLongINT(evt, input) {
                var charCode;
                if (evt.keyCode) //For IE
                    charCode = evt.keyCode;
                else if (evt.Which)
                    charCode = evt.Which; // For FireFox
                else
                    charCode = evt.charCode; // Other Browser
                if (charCode > 31 && (charCode < 48 || charCode > 57))
                    return false;
                else
                    return true;
            }
            function ValidateTextBoxForDataTypeTextAlphaNumericFileNumber(evt, value, input, lbltext) {
                //CommaSpaceHypen  {}-@,.~!%$()*/
                var charCode;
                if (evt.keyCode) //For IE
                    charCode = evt.keyCode;
                else if (evt.Which)
                    charCode = evt.Which; // For FireFox
                else
                    charCode = evt.charCode; // Other Browser
                if ((charCode >= 48 && charCode <= 57) || (charCode > 64 && charCode <= 90) || (charCode >= 97 && charCode <= 122) || (charCode == 08 || charCode == 32 || charCode == 45 || charCode == 46)) {
                    return true;
                }

                else
                    return false;

            }

            function validateKeyup(input) {
                var val = input.value.replace(/^[\s!@#$%^&*(){}|+_)~`<>,.]+|\s+$/, '');

                if (val.length == 0) {
