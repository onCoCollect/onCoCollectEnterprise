    <input type="hidden" id="logoutstat" value="" />

    <script type="text/javascript">
        var window_focus;
        $(window).focus(function () {
            window_focus = true;
            if (window_focus = true) {
                var logstat = $('#logoutstat').val();
                if (logstat === '0') {
                    //alert('You have logged out!');
                    window.location = "Login.aspx";
                }
                doCheckLoggedOut();
            }
        }).blur(function () {
            window_focus = false;
        });

        function doCheckLoggedOut() {
            $.ajax(
                {
                    type: "POST",
                    url: "SearchPatient.aspx/doCheckLoggedOut",
                    data: "{}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    async: true,
                    cache: false,
                    success: function (msg) {
                        var logstat = msg.d;
                        $('#logoutstat').val(logstat);
                        var elems = msg.d.toString().split(':');
                        if (elems[0] === '0' && elems[1] === '1' && elems[2] === '0') { //elems[1] live updatebit
                            alert('Now you have been Logged out as the System is being upgraded by the System Administrator.');
                            window.location = "Login.aspx";
                        }
                        else if (elems[0] === '0' && elems[1] === '0' && elems[2] === '1') { //elems[2] reset password
                            alert('You have been force logged out, because your password has been resetted by Administrator.');
                            window.location = "Login.aspx";
                        }
                        else if (elems[0] === '0' && elems[3] === '1') { //elems[4]  Deactivate user Accourt
                            alert('Now you have been Logged out , as the System Administrator has deactive your account.');
                            window.location = "Login.aspx";
                        }
                        else if (elems[0] === '0') {
                            alert('You have been force Logged out, as the same user is trying to Log In.');
                            window.location = "Login.aspx";
                        }
                       
                    },
                    error: function (x, e) {
                        // alert("The call to the server side failed. doCheckLoggedOut. " + x.responseText);
                    }
                }
            );
        }
    </script>
