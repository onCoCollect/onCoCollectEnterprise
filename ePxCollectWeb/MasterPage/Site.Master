    <input type="hidden" id="logoutstat" value="" />

    <script type="text/javascript">
        //onCoDatePik
        $(function () {
            $(document).on("focus", ".onCoDatePik", function () {
                $(".onCoDatePik").datepicker({
                    changeMonth: true,
                    changeYear: true,
                    dateFormat: "MM d, yy",
                    showAnim: 'slideDown',
                    yearRange: '1914:2015',
                    minDate: new Date(1914, 10 - 1, 25),
                    maxDate: "+1M +10D",
                    showButtonPanel: true,
                    closeText: 'Clear',
                    onClose: function (dateText, inst) {
                        if ($(window.event.srcElement).hasClass('ui-datepicker-close'))
                            $(this).closest(".onCoDatePik").val('');
                    }
                    //showOn: 'button',
                    //buttonText: 'Show Date',
                    //buttonImageOnly: true,
                    //buttonImage: '~/images/Calendar.png',
                });
            });

            $(document).on("focus", ".onCoDatePik_uptotoday", function () {
                $(".onCoDatePik_uptotoday").datepicker({
                    changeMonth: true,
                    changeYear: true,
                    dateFormat: "MM d, yy",
                    showAnim: 'slideDown',
                    minDate: "-100Y",
                    maxDate: 0,
                    showButtonPanel: true,
                    closeText: 'Clear',
                    onClose: function (dateText, inst) {
                        if ($(window.event.srcElement).hasClass('ui-datepicker-close'))
                            $(this).closest(".onCoDatePik_uptotoday").val('');
                    }
                    //showOn: 'button',
                    //buttonText: 'Show Date',
                    //buttonImageOnly: true,
                    //buttonImage: '~/images/Calendar.png',
                });
            });

            //$('button.ui-datepicker-current').live('click', function () {
            //    $.datepicker._curInst.input.datepicker('setDate', new Date());
            //});

            //$('.onCoDatePik').datepicker().on('changeDate', function () {
            //    $(this).blur();
            //});
        });

        var window_focus;
        $(window).focus(function () {
            window_focus = true;
            if (window_focus = true) {
                var logstat = $('#logoutstat').val();
                if (logstat === '0') {
                    //alert('You have logged out!');
                    window.location = "Login.aspx";
                }
                doCheckLoggedOut();
            }
        }).blur(function () {
            window_focus = false;
        });

        function doCheckLoggedOut() {
            $.ajax(
                {
                    type: "POST",
                    url: "SearchPatient.aspx/doCheckLoggedOut",
                    data: "{}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    async: true,
                    cache: false,
                    success: function (msg) {
                        var logstat = msg.d;
                        $('#logoutstat').val(logstat);
                        var elems = msg.d.toString().split(':');
                        if (elems[0] === '0' && elems[1] === '1' && elems[2] === '0') { //elems[1] live updatebit
                            //alert('Now you have been Logged out as the System is being upgraded by the System Administrator.');
                            showErrMsg('Now you have been Logged out as the System is being upgraded by the System Administrator.');
                            window.location = "Login.aspx";
                        }
                        else if (elems[0] === '0' && elems[1] === '0' && elems[2] === '1') { //elems[2] reset password
                            //alert('You have been force logged out, because your password has been resetted by Administrator.');
                            showErrMsg('You have been force logged out, because your password has been resetted by Administrator.');
                            window.location = "Login.aspx";
                        }
                        else if (elems[0] === '0' && elems[3] === '1') { //elems[4]  Deactivate od Accourt
                            //alert('Now you have been Logged out , as the System Administrator has deactive your account.');
                            showErrMsg('Now you have been Logged out , as the System Administrator has deactive your account.');
                            window.location = "Login.aspx";
                        }
                        else if (elems[0] === '0') {
                            alert('You have been force Logged out, as the same user is trying to Log In.');
                            showErrMsg('You have been force Logged out, as the same user is trying to Log In.');
                            window.location = "Login.aspx";
                        }

                    },
                    error: function (x, e) {
                        //alert("The call to the server side failed. doCheckLoggedOut. " + x.responseText);
                    }
                }
            );
        }

        function resetNotifyCss() {
            $("#toggleCSS").attr("href", "~/Style/alertify.default.css");
            alertify.set({
                labels: {
                    ok: "OK",
                    cancel: "Cancel"
                },
                delay: 2000,
                buttonReverse: false,
                buttonFocus: "ok"
            });
        }

        //noti_Message -> Message to display;  msgType -> Type of Message(S - success, E - Error, I - Information)
        function oncoNotify(noti_Message, msgType) {
            resetNotifyCss();
            if (msgType == "S") {                
                alertify.success(noti_Message);
                setTimeout(function () {
                    window.location.reload();
                }, 2000);                
            }
            else
                if (msgType == "E") {
                    alertify.error(noti_Message);
                }
                else {
                    alertify.custom = alertify.extend("custom");
                    alertify.custom(noti_Message);
                }
            return false;
        }
    </script>

