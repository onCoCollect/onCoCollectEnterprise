            function checkChangePassword(id) {

                var obj = document.getElementById(id);

                if (!checkEmpty('ctl00_MainContent_txtCurrentPassword', 'Fields marked with asterisk (*) are required.')) {

                    document.getElementById('ctl00_MainContent_txtCurrentPassword').value = '';

                    return false;

                }

                if (!CheckPassword('ctl00_MainContent_txtCurrentPassword')) { // check to make sure all characters are valid

                    document.getElementById('ctl00_MainContent_lblmsg').innerHTML = "Current Password should have at least one numeric digit and a special character (Password Length:8-15).";

                    document.getElementById('ctl00_MainContent_txtCurrentPassword').value = '';

                    document.getElementById('ctl00_MainContent_txtCurrentPassword').focus();

                    return false;

                }

                if (!checkEmpty('ctl00_MainContent_txtNewPassword', 'Fields marked with asterisk (*) are required.')) {

                    document.getElementById('ctl00_MainContent_txtNewPassword').value = '';

                    return false;

                }

                if (!CheckPassword('ctl00_MainContent_txtNewPassword')) { // check to make sure all characters are valid

                    document.getElementById('ctl00_MainContent_lblmsg').innerHTML = "New password should have at least one numeric digit and a special character (Password Length:8-15).";

                    document.getElementById('ctl00_MainContent_txtNewPassword').value = '';

                    document.getElementById('ctl00_MainContent_txtNewPassword').focus();

                    return false;

                }

                if (document.getElementById('ctl00_MainContent_txtCurrentPassword').value == document.getElementById('ctl00_MainContent_txtNewPassword').value) {

                    document.getElementById('ctl00_MainContent_lblmsg').innerHTML = "New password should not be same as current password.";/*Code remodified on March 10-2015,Subhashini*/

                    document.getElementById('ctl00_MainContent_txtNewPassword').value = '';/*Code added on March 10-2015,Subhashini*/

                    document.getElementById('ctl00_MainContent_txtConfirmPassword').value = '';

                    document.getElementById('ctl00_MainContent_txtNewPassword').focus();/*Code modified on March 10-2015,Subhashini*/

                    return false;

                }

                if (!checkEmpty('ctl00_MainContent_txtConfirmPassword', 'Fields marked with asterisk (*) are required.')) {

                    document.getElementById('ctl00_MainContent_txtConfirmPassword').value = '';

                    return false;

                }

                if (!CheckPassword('ctl00_MainContent_txtConfirmPassword')) { // check to make sure all characters are valid

                    document.getElementById('ctl00_MainContent_lblmsg').innerHTML = "Confirm password should have at least one numeric digit and a special character (Password Length:8-15).";

                    document.getElementById('ctl00_MainContent_txtConfirmPassword').value = '';

                    document.getElementById('ctl00_MainContent_txtConfirmPassword').focus();

                    return false;

                }

                if (document.getElementById('ctl00_MainContent_txtNewPassword').value != document.getElementById('ctl00_MainContent_txtConfirmPassword').value) {

                    document.getElementById('ctl00_MainContent_lblmsg').innerHTML = "Confirm password should match with new password.";

                    document.getElementById('ctl00_MainContent_txtConfirmPassword').value = '';

                    document.getElementById('ctl00_MainContent_txtConfirmPassword').focus();

                    return false;

                }

                if (!checkEmpty('ctl00_MainContent_ddlPasswordHintQuestion', 'Fields marked with asterisk (*) are required.'))

                    return false;

                if (!checkEmpty('ctl00_MainContent_txtAnswer', 'Fields marked with asterisk (*) are required.')) {

                    document.getElementById('ctl00_MainContent_txtAnswer').value = '';

                    document.getElementById('ctl00_MainContent_txtAnswer').focus();

                    return false;

                }

                if (!allValidPasswordChars('ctl00_MainContent_txtAnswer')) { // check to make sure all characters are valid

                    document.getElementById('ctl00_MainContent_lblmsg').innerHTML = 'Enter valid Security Answer.';

                    document.getElementById('ctl00_MainContent_txtAnswer').value = '';

                    document.getElementById('ctl00_MainContent_txtAnswer').focus();

                    return false;

                }

                if (!checkEmpty('ctl00_MainContent_txtEmail', 'Fields marked with asterisk (*) are required.'))

                    return false;

                if (document.getElementById('ctl00_MainContent_txtEmail').value.length > 0) {

                    if (!isValidEmail(document.getElementById('ctl00_MainContent_txtEmail'))) {

                        document.getElementById('ctl00_MainContent_lblmsg').innerHTML = 'Enter valid EmailID.';

                        document.getElementById('ctl00_MainContent_txtEmail').value = '';

                        document.getElementById('ctl00_MainContent_txtEmail').focus();

                        return false;

                    }

                }

                if (!checkEmpty('ctl00_MainContent_txtExtension', 'Fields marked with asterisk (*) are required.'))

                    return false;

                if (document.getElementById('ctl00_MainContent_txtExtension').value.length < 2) {

                    document.getElementById('ctl00_MainContent_lblmsg').innerHTML = "Enter valid Mobile Extension.";

                    document.getElementById('ctl00_MainContent_txtExtension').value = '';

                    document.getElementById('ctl00_MainContent_txtExtension').focus();

                    return false;

                }

                if (!checkEmpty('ctl00_MainContent_txtPhone', 'Fields marked with asterisk (*) are required.'))

                    return false;

                if (document.getElementById('ctl00_MainContent_txtPhone').value.length < 10) {

                    document.getElementById('ctl00_MainContent_lblmsg').innerHTML = "Enter valid Mobile Number.";

                    document.getElementById('ctl00_MainContent_txtPhone').value = '';

                    document.getElementById('ctl00_MainContent_txtPhone').focus();

                    return false;

                }

                return true;
            }



