            //Composite Form
            function fnPopWindow(fName, ColName, ctlname) {
                var $iframe = $('#onCoRightPickDiag_iframe');
                var url = fName;
                if ($iframe.length) {
                    $iframe.attr('src', url);
                    $("#onCoRightPickDiag").dialog(
                        {
                            modal: true,
                            width: '60%',
                            minHeight: '400',
                            resizable: false,
                            stack: true,
                            title: 'Select',
                            buttons: {
                                "OK": function () {
                                    $("#onCoRightPickDiag_iframe")[0].contentWindow.getValFromDialog(ctlname);
                                },
                                "Close": function () {
                                    $(this).dialog('close');
                                }
                            },
                            show: {
                                effect: "scale",
                                duration: "500",
                            },
                            hide: {
                                effect: "fade",
                                duration: "500",
                            }
                        }
                        );
                    return false;
                }
                return true;
            }

            //Replace &+ PopupMultiSelect using JQuery Dialog
            function fnPopup(tName, ctlName, strVal) {

                var ctl = document.getElementById(ctlName);

                if (ctl != null) {
                    strVal = document.getElementById(ctlName).value.toString();
                }

                strVal = strVal.replace(/&/gi, '*ampersand*')
                strVal = strVal.replace(/\+/gi, '*plus*')

                var $iframe = $('#onCoRightPickDiag_iframe');
                var url = "UserControl/PopupMultiSelect.aspx?tName=" + tName + "&Val=" + strVal;
                if ($iframe.length) {
                    $iframe.attr('src', url);
                    $("#onCoRightPickDiag").dialog(
                        {
                            modal: true,
                            width: '60%',
                            minHeight: '400',
                            resizable: false,
                            stack: true,
                            title: 'Select',
                            buttons: {
                                "OK": function () {
                                    var retVal = $("#onCoRightPickDiag_iframe")[0].contentWindow.CloseDialog();
                                    try {
                                        if (retVal == null) {
                                            document.getElementById(ctlName).value = document.getElementById(ctlName).value;
                                            //alert(retVal);
                                        } else
                                            if (retVal == undefined) {
                                                document.getElementById(ctlName).value = document.getElementById(ctlName).value;
                                                //alert(retVal );
                                            } else
                                                if (retVal != "") {
                                                    document.getElementById(ctlName).value = retVal.toString();
                                                }
                                                else
                                                    if (retVal == "") {
                                                        document.getElementById(ctlName).value = "";
                                                    }

                                        if (ctlName == "ctl00_MainContent_2nd_-Surgery_RTX_Y") {
                                            __doPostBack("ctl00_MainContent_2nd_-Surgery_RTX_Y", "TextChanged");
                                        }
                                    }
                                    catch (e) {
                                    }

                                    $(this).dialog('close');
                                },
                                "Close": function () {
                                    $(this).dialog('close');
                                }
                            },
                            show: {
                                effect: "scale",
                                duration: "500",
                            },
                            hide: {
                                effect: "fade",
                                duration: "500",
                            }
                        }
                        );
                    return false;
                }
                return true;



            }

